# Required: Choose your AI model provider
MODEL_PROVIDER=openai  # Options: openai, anthropic, grok, groq, llama_cloud, llama_local, google, claude_vertex, redpill, openrouter, ollama

# Required for testing
TEST_DATABASE_CLIENT=sqlite
NODE_ENV=test

# Optional: OpenAI configuration (if using OpenAI)
OPENAI_API_KEY=your_openai_key

# Optional: Voice generation (if using voice features)
ELEVENLABS_API_KEY=your_elevenlabs_key
ELEVENLABS_VOICE_ID=your_voice_id
VOICE_STABILITY=0.5
VOICE_SIMILARITY=0.75

# Optional: Local model settings (if using local LLM)
MODEL_PATH=./models/your_model.gguf 